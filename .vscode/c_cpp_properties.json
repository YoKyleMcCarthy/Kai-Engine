{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/windows/includes/**",
                "${workspaceFolder}/windows/includes/*/**",
                "${workspaceFolder}/Engine/src/**"
            ],
            "defines": [
                "_DEBUG",
                "KAI_EXPORT"
            ],
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "${default}"
        },
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/mac/includes/**",
                "${workspaceFolder}/Engine/src/**"
            ],
            "defines": [
                "_DEBUG",
                "KAI_EXPORT"
            ],
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "${default}"
        }
    ],
    "version": 4
}